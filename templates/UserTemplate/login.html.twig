{% extends 'base.html.twig' %}

{% block body %}

    {% for flash_message in app.flashes('message') %}
        <div class="flash-notice">
            {{ flash_message }}
        </div>
    {% endfor %}

    <h1>Contact Form</h1>
    {{ form_start(form, {"action" : "", "method" : "POST"}) }}
    {{ form_widget(form) }}
    <button type="submit">Submit</button>
    {{ form_end(form) }}

{% endblock %}